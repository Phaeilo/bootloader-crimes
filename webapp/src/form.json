{
  "formgroups": [
    {
      "label": "Machine Settings",
      "inputs": [
        {
          "name": "vm_name",
          "label": "VM Name",
          "default": "winvm"
        },
        {
          "name": "vm_ram",
          "label": "RAM Size (MB)",
          "default": "4096"
        },
        {
          "name": "vm_cores",
          "label": "CPU Cores",
          "default": "4"
        }
      ]
    },

    {
      "label": "Install Media",
      "inputs": [
        {
          "name": "iso_url",
          "label": "ISO URL",
          "default": "https://go.microsoft.com/fwlink/p/?LinkID=2206317&clcid=0x409&culture=en-us&country=US"
        },
        {
          "name": "iso_hash",
          "label": "ISO SHA256",
          "default": "c8dbc96b61d04c8b01faf6ce0794fdf33965c7b350eaa3eb1e6697019902945c"
        },
        {
          "name": "iso_wim_index",
          "label": "WIM Index",
          "default": "1"
        }
      ]
    },

    {
      "label": "System Settings",
      "inputs": [
        {
          "name": "ua_hostname",
          "label": "Hostname",
          "default": "winvm"
        },
        {
          "name": "ua_username",
          "label": "Username",
          "default": "vmuser"
        },
        {
          "name": "ua_password",
          "label": "Password",
          "default": "vmuser"
        },
        {
          "name": "ua_autologon",
          "label": "Auto-logon",
          "type": "checkbox"
        },
        {
          "name": "ua_timezone",
          "label": "Timezone",
          "type": "select",
          "options": "TIMEZONE",
          "default": "UTC"
        },
        {
          "name": "ua_input_locale",
          "label": "Keyboard Layout",
          "type": "select",
          "options": "LOCALE",
          "default": "en-US"
        },
        {
          "name": "ua_system_locale",
          "label": "System Locale",
          "type": "select",
          "options": "LOCALE",
          "default": "en-US"
        },
        {
          "name": "ua_user_locale",
          "label": "User Locale",
          "type": "select",
          "options": "LOCALE",
          "default": "en-US"
        },
        {
          "name": "ua_system_language",
          "label": "System Language",
          "type": "select",
          "options": "LOCALE",
          "default": "en-US"
        }
